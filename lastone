#include <iostream>

using namespace std;

#include<iostream>
using namespace std;

int main(){
  int t,n,k=1;
  cin>>t;
  while(k<=t){
    cin>>n;
    int a[n],index=-1;
    for(int i=0;i<n;i++){
      cin>>a[i];
    }
    for(int i=0;i<=(n-1);i++){
      if(a[i]==1){
        index=i;
      }
    }
    cout<<index<<endl;
    k++;
  }
  
  
}
